<miruken:View x:Class="Test_App.Features.Documentation.Documentation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Test_App.Features.Documentation"
             xmlns:miruken="clr-namespace:Miruken.Mvc.Wpf;assembly=Miruken.Mvc.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="300">
    <Grid Background="White">
        <TabControl x:Name="tabControl" Margin="10,10,10,42">
            <TabItem Header="Core">
                <Grid Background="#FFE5E5E5" Margin="0,0,0,2"/>
            </TabItem>
            <TabItem Header="Callback">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Context" Margin="0,0,-4,-2">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Button x:Name="button" Command="{miruken:Action Done}" Content="Done" Margin="0,0,10,10" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="61"/>
    </Grid>
</miruken:View>



